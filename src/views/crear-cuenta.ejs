            <label>E-mail:</label>
            <input type="text" name="email" placeholder="E-mail">
        </div>
        <div class="campo">
            <label>Nombre:</label>
            <input type="text" name="nombre" placeholder="Nombre">
        </div>
        <div class="campo">
            <label>Contrase単a:</label>
            <input type="password" name="password" placeholder="Contrase単a">
        </div>
        <div class="campo">
            <label>Repetir Contrase単a:</label>
            <input type="password" name="repetir_password" placeholder="Confirmar Contrase単a">
        </div>
        <div class="campo enviar">
            <input type="submit" class="btn btn-rosa" value="Crear Cuenta">
        </div>
    </form>
</main>